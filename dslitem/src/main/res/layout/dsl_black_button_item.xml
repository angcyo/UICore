<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lib_item_root_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/lib_padding_left"
    android:paddingTop="@dimen/lib_hdpi"
    android:paddingRight="@dimen/lib_padding_left"
    android:paddingBottom="@dimen/lib_hdpi">

    <com.angcyo.widget.DslButton
        android:id="@+id/lib_button"
        style="@style/LibButtonBlackSolidStyle"
        android:layout_width="match_parent"
        android:layout_height="@dimen/lib_item_min_height"
        android:layout_marginHorizontal="@dimen/lib_xhdpi"
        tools:text="@string/dialog_positive" />

</FrameLayout>