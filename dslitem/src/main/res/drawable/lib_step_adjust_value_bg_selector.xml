<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="@dimen/lib_step_adjust_stroke_width" android:color="@color/lib_step_adjust_tint_color" />
            <solid android:color="@color/lib_disable_bg_color" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="@dimen/lib_step_adjust_stroke_width" android:color="@color/lib_step_adjust_tint_color" />
            <solid android:color="@color/lib_bg_press" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="@dimen/lib_step_adjust_stroke_width" android:color="@color/lib_step_adjust_tint_color" />
        </shape>
    </item>
</selector>