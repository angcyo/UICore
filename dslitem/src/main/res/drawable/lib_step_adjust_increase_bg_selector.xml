<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:bottomRightRadius="@dimen/lib_step_adjust_radius" android:topRightRadius="@dimen/lib_step_adjust_radius" />
            <solid android:color="@color/lib_disable_bg_color" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:bottomRightRadius="@dimen/lib_step_adjust_radius" android:topRightRadius="@dimen/lib_step_adjust_radius" />
            <solid android:color="@color/lib_bg_press" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/transparent" />
        </shape>
    </item>
</selector>